<Window x:Class="ojm.Views.DeliveryView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="DeliveryView" Height="300" Width="277" WindowStartupLocation="CenterScreen">
    <Grid>
        <DatePicker x:Name="DeliveryDate" HorizontalAlignment="Center" Margin="0,67,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="Quantity" Width="102" Height="25" HorizontalAlignment="Center" Margin="0,122,0,126.424"/>
        <CheckBox Content="Ankommet" Name="Arrived" HorizontalAlignment="Center" Margin="0,164,0,0" VerticalAlignment="Top" Checked="CheckBox_Checked"/>
        <Button x:Name="Save" Content="Gem" HorizontalAlignment="Center" Margin="148,201,53.916,0" VerticalAlignment="Top" Width="70" Click="Save_Click"/>
        <Button x:Name="Cancel" Content="Annuller" HorizontalAlignment="Center" Margin="56,201,145.916,0" VerticalAlignment="Top" Width="70" Click="Cancel_Click"/>
        <Label x:Name="MaterialName" Content="" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" FontSize="20"/>
        <Label Content="Tilføj materialeordre" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,10,0,0" FontWeight="Bold"/>
        <Label Content="Ankomst" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,41,0,0"/>
        <Label Content="Antal" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,96,0,0"/>
    </Grid>
</Window>
